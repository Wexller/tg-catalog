{
  "name": "tg-catalog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --no-fix ./src && eslint --no-fix ./types && vue-tsc --noEmit",
    "lint:fix": "eslint src --fix && eslint types --fix",
    "typecheck": "vue-tsc --noEmit",
    "predeploy": "vue-tsc && vite build",
    "deploy": "gh-pages -d build",
    "github": "\"yarn deploy\""
  },
  "dependencies": {
    "tailwind-variants": "^0.1.13",
    "tailwindcss": "^3.3.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-tg": "^0.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-vue": "^9.17.0",
    "gh-pages": "^6.0.0",
    "postcss": "^8.4.28",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
